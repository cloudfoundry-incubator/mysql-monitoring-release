    unit: page
  innodb_buffer_pool_pages_total:
    key: /<%= p("mysql-metrics.origin") %>/innodb/buffer_pool_pages_total
    unit: page
  innodb_buffer_pool_pages_data:
    key: /<%= p("mysql-metrics.origin") %>/innodb/buffer_pool_pages_data
    unit: page
  innodb_row_lock_current_waits:
    key: /<%= p("mysql-metrics.origin") %>/innodb/row_lock_current_waits
    unit: lock
  innodb_data_read:
    key: /<%= p("mysql-metrics.origin") %>/innodb/data_read
    unit: byte
  innodb_data_written:
    key: /<%= p("mysql-metrics.origin") %>/innodb/data_written
    unit: byte
  innodb_mutex_os_waits:
    key: /<%= p("mysql-metrics.origin") %>/innodb/mutex_os_waits
    unit: event
  innodb_mutex_spin_rounds:
    key: /<%= p("mysql-metrics.origin") %>/innodb/mutex_spin_rounds
    unit: event
  innodb_mutex_spin_waits:
    key: /<%= p("mysql-metrics.origin") %>/innodb/mutex_spin_waits
