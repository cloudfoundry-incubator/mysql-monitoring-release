---
internal_metrics:
  available:
    key: /<%= p("mysql-metrics.origin") %>/available
leader_follower_metrics:
  is_follower:
    key: /<%= p("mysql-metrics.origin") %>/follower/is_follower
    unit: integer
  seconds_behind_master:
    key: /<%= p("mysql-metrics.origin") %>/follower/seconds_behind_master
    unit: integer
  relay_log_space:
    key: /<%= p("mysql-metrics.origin") %>/follower/relay_log_space
    unit: bytes
  slave_io_running:
    key: /<%= p("mysql-metrics.origin") %>/follower/slave_io_running
    unit: boolean
  slave_sql_running:
    key: /<%= p("mysql-metrics.origin") %>/follower/slave_sql_running
    unit: boolean
mysql_metrics:
  max_connections:
    key: /<%= p("mysql-metrics.origin") %>/variables/max_connections
    unit: integer
  open_files_limit:
    key: /<%= p("mysql-metrics.origin") %>/variables/open_files_limit
    unit: integer
  questions:
    key: /<%= p("mysql-metrics.origin") %>/performance/questions
    unit: metric
  innodb_buffer_pool_pages_free:
