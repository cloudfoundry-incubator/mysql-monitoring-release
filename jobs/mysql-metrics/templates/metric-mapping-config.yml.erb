  innodb_row_lock_time:
    key: /<%= p("mysql-metrics.origin") %>/innodb/row_lock_time
    unit: millisecond
  innodb_row_lock_waits:
    key: /<%= p("mysql-metrics.origin") %>/innodb/row_lock_waits
    unit: event
  connections:
    key: /<%= p("mysql-metrics.origin") %>/net/connections
    unit: connection
  max_used_connections:
    key: /<%= p("mysql-metrics.origin") %>/net/max_used_connections
    unit: connection
  com_delete:
    key: /<%= p("mysql-metrics.origin") %>/performance/com_delete
    unit: query
  com_delete_multi:
    key: /<%= p("mysql-metrics.origin") %>/performance/com_delete_multi
    unit: query
  com_insert:
    key: /<%= p("mysql-metrics.origin") %>/performance/com_insert
    unit: query
  com_insert_select:
    key: /<%= p("mysql-metrics.origin") %>/performance/com_insert_select
    unit: query
  com_replace_select:
    key: /<%= p("mysql-metrics.origin") %>/performance/com_replace_select
    unit: query
  com_select:
    key: /<%= p("mysql-metrics.origin") %>/performance/com_select
    unit: query
  com_update:
    key: /<%= p("mysql-metrics.origin") %>/performance/com_update
    unit: query
  com_update_multi:
    key: /<%= p("mysql-metrics.origin") %>/performance/com_update_multi
    unit: query
