  created_tmp_files:
    key: /<%= p("mysql-metrics.origin") %>/performance/created_tmp_files
    unit: file
  created_tmp_tables:
    key: /<%= p("mysql-metrics.origin") %>/performance/created_tmp_tables
    unit: table
  cpu_time:
    key: /<%= p("mysql-metrics.origin") %>/performance/cpu_time
    unit: second
  open_files:
    key: /<%= p("mysql-metrics.origin") %>/performance/open_files
