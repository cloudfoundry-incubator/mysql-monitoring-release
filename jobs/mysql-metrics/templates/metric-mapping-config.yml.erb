  slow_queries:
    key: /<%= p("mysql-metrics.origin") %>/performance/slow_queries
    unit: query
  table_locks_waited:
    key: /<%= p("mysql-metrics.origin") %>/performance/table_locks_waited
    unit: number
  threads_connected:
    key: /<%= p("mysql-metrics.origin") %>/performance/threads_connected
    unit: connection
  threads_running:
    key: /<%= p("mysql-metrics.origin") %>/performance/threads_running
    unit: thread
  busy_time:
    key: /<%= p("mysql-metrics.origin") %>/performance/busy_time
    unit: second
<% if p("mysql-metrics.galera_metrics_enabled") %>
  wsrep_cluster_size:
    key: /<%= p("mysql-metrics.origin") %>/galera/wsrep_cluster_size
    unit: node
  wsrep_local_recv_queue:
    key: /<%= p("mysql-metrics.origin") %>/galera/wsrep_local_recv_queue
    unit: float
  wsrep_local_send_queue:
    key: /<%= p("mysql-metrics.origin") %>/galera/wsrep_local_send_queue
    unit: float
  wsrep_local_index:
    key: /<%= p("mysql-metrics.origin") %>/galera/wsrep_local_index
    unit: float
  wsrep_local_state:
    key: /<%= p("mysql-metrics.origin") %>/galera/wsrep_local_state
    unit: float
  wsrep_ready:
    key: /<%= p("mysql-metrics.origin") %>/galera/wsrep_ready
    unit: number
  wsrep_cluster_status:
    key: /<%= p("mysql-metrics.origin") %>/galera/wsrep_cluster_status
    unit: number
<% end %>
